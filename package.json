{
	"name": "REPLACE_ME",
	"version": "0.1.0",
	"type": "module",
	"private": true,
	"bin": {
		"REPLACE_ME": "./src/cli.ts"
	},
	"scripts": {
		"cli": "bun run src/cli.ts",
		"dev": "bun run --watch src/cli.ts",
		"lint": "biome check .",
		"lint:fix": "biome check --write .",
		"format": "biome format --write . && prettier --write '**/*.md'",
		"check": "tsc --noEmit && biome check .",
		"typecheck": "tsc --noEmit",
		"test": "bun test"
	},
	"dependencies": {
		"chalk": "^5.6.2",
		"cmd-ts": "^0.13.0",
		"pino": "^10.3.0",
		"pino-pretty": "^13.1.3",
		"zod": "^4.0.0"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.3.13",
		"@types/bun": "latest",
		"biome-plugin-no-type-assertion": "^1.0.2",
		"prettier": "^3.8.1",
		"typescript": "^5.7.3"
	}
}
